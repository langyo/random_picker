{
  "name": "random_picker",
  "version": "1.0.0",
  "description": "给物理老师写的专用课堂点名器！",
  "main": "public/bundle.js",
  "scripts": {
    "watch": "watchify ./scripts/mainViewRender.js -t babelify -o ./public/bundle.js --release --verbose",
    "build": "browserify ./scripts/mainViewRender.js -o ./public/js/bundle.js -t babelify",
    "start": "electron ./index.js --inspect=5858",
    "test": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/langyo/random_picker.git"
  },
  "author": "langyo",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/langyo/random_picker/issues"
  },
  "homepage": "https://github.com/langyo/random_picker#readme",
  "dependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "@material-ui/core": "^3.9.2",
    "@material-ui/system": "^3.0.0-alpha.2",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.0-beta.0",
    "classnames": "^2.2.6",
    "lowdb": "^1.0.0",
    "mdi-material-ui": "^5.8.0",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "reflux": "^6.4.1",
    "regenerator-runtime": "^0.13.1",
    "uglifyjs": "^2.4.11",
    "shortid": "^2.2.14",
    "typescript": "^2.8.3",
    "watchify": "^3.11.1",
    "webpack": "^4.26.1"
  },
  "keywords": [],
  "devDependencies": {
    "babelify": "^10.0.0",
    "browserify": "^16.2.3",
    "electron": "^3.0.10",
    "electron-builder": "^20.38.5",
    "electron-packager": "^13.0.1",
    "jest": "^24.1.0"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": "commonjs",
          "targets": {
            "node": "current"
          }
        }
      ],
      [
        "@babel/preset-react"
      ]
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-class-properties"
      ]
    ]
  }
}
